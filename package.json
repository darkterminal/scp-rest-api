{
  "name": "@darkterminal/eas-es6",
  "version": "1.2.2",
  "description": "A basic starter for an express.js API using ES6",
  "bin": "./bin/cli.js",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node src/index.js",
    "build": "rimraf build && babel src --out-dir build --copy-files",
    "start": "node build/index.js",
    "lint": "eslint --fix src",
    "test": "mocha --exit"
  },
  "keywords": [
    "express es6 api starter",
    "express",
    "es6"
  ],
  "author": "CJ R. <cj@null.computer> (https://github.com/w3cj/express-api-starter.git)",
  "repository": {
    "type": "git",
    "url": "https://github.com/darkterminal/express-api-starter.git"
  },
  "engines": {
    "node": ">14 <18"
  },
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^19.0.7",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.4",
    "mocha": "^9.0.0",
    "nodemon": "^2.0.7",
    "rimraf": "^4.0.7",
    "supertest": "^6.1.3"
  }
}
